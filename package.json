{
  "name": "trek-tips",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-ios": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev-ios",
    "start-android": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev-android",
    "start:prod": "node server.js",
    "start:dev-ios": "concurrently \"nodemon --ignore 'app/*'\" \"npm run app-ios\"",
    "start:dev-android": "concurrently \"nodemon --ignore 'app/*'\" \"npm run app-android\"",
    "app-ios": "cd app && react-native run-ios",
    "app-android": "cd app && react-native run-android",
    "postinstall": "cd app && npm install",
    "populate": "babel-cli/bin/babel-node.js populate.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parkmee/trek-tips.git"
